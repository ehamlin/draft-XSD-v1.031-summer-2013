	<xs:simpleType name="event_result"><!-- this is a master list of all result types that can be returned as the "result" element of an event_exception record --><!--for speed, the error code is prepended to the message with a pipe sign-->
		<xs:restriction base="xs:string">
			<xs:enumeration value="000|Success"/>
			<xs:enumeration value="100|Unknown event type"/>
			<xs:enumeration value="101|Unknown partner system"/>
			<xs:enumeration value="102|Malformed event record"/>
			<xs:enumeration value="103|Missing/incorrect event_data payload"/>
			<xs:enumeration value="104|Action requested is not valid for the specified event"/>
			<xs:enumeration value="105|Event publisher has insufficient rights to raise the event"/>
			<xs:enumeration value="106|Event publisher has insufficient rights to request the specified Action"/>
			<xs:enumeration value="107|Unsupported event"/>
			<xs:enumeration value="108|Unintended event echo"/>
			<xs:enumeration value="200|Orchestration error"/>
			<xs:enumeration value="201|Orchestration timeout"/>
			<xs:enumeration value="202|External web service unavailable"/>
			<xs:enumeration value="203|Expected batch file not found"/>
			<xs:enumeration value="204|External web service exception"/>
			<xs:enumeration value="300|Person matching exception: expected singleton match, no match found"/>
			<xs:enumeration value="301|Person matching exception: expected singleton match, multiple matches found"/>
			<xs:enumeration value="302|Person matching exception: target system or data unavailable within timeout period"/>
			<xs:enumeration value="303|Attempt to create a cross-system person mapping where one already exists"/>
			<xs:enumeration value="304|Attempt to break a cross-system person mapping that does not exist"/>
			<xs:enumeration value="305|Requested person not found"/>
			<xs:enumeration value="306|Requested case not found"/>
			<xs:enumeration value="307|Record with the specified ID not found"/> <!-- 1.031 addition -->
			<xs:enumeration value="401|Null person ID"/>
			<xs:enumeration value="402|Null case ID"/>
			<xs:enumeration value="403|Null assessment ID"/>
			<xs:enumeration value="404|Null resource ID"/>
			<xs:enumeration value="405|Null placement ID"/>
			<xs:enumeration value="406|Null worker ID"/>
			<xs:enumeration value="499|Missing legacy ID"/>
			<xs:enumeration value="501|Illegal date value"/>
			<xs:enumeration value="502|Invalid binary content"/>
			<xs:enumeration value="601|Invalid address update request: Focus child has moved" />  <!-- 1.031 addition -->
			<xs:enumeration value="602|Invalid address update request: Address record not found" />  <!-- 1.031 addition -->
			<xs:enumeration value="603|Invalid address content" />  <!-- 1.031 addition -->
			<xs:enumeration value="700|Business rule violation" />  <!-- 1.031 addition -->
			<xs:enumeration value="701|An [add] action may not reference an existing record; the item ID cannot exist in Casebook" />  <!-- 1.031 addition -->
			<xs:enumeration value="800|Attachment error - unclassified" />
			<xs:enumeration value="801|Incoming attachment must be linked to a Casebook unit of work; ID source for the associated entity ID was not 'casebook'"/>
			<xs:enumeration value="802|Missing attachment filename"/>
			<xs:enumeration value="803|Neither a URI to a file nor a binary copy of the file was provided for the incoming attachment" />
			<xs:enumeration value="804|Could not create attachment in Casebook: the binary content sent was corrupt" />
